&i2c0 {
	status = "okay";
	clock-frequency = <I2C_BITRATE_STANDARD>;
	as7331@74 {
		compatible = "ams,as7331";
		reg = <0x74>;
		// ready-gpios = <&gpio0 11 (GPIO_ACTIVE_HIGH)>;
		ready-gpios = <&gpio0 11 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
		// sync-gpios = <&gpio0 20 (GPIO_ACTIVE_HIGH)>;
	};
};
